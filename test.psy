== build ==
{
	add_source_directory("src");
	set_executable("stdtest");
	set_optimization(0);
}

mystruct ::= struct
{
	x : u64;
};

main ::= func(-> s64)
{
	s ::= zero@mystruct mut;
	sptr ::= ref s;
	putuint(s.x);
	putchar(10);

	sptr->x = 5;

	putuint(s.x);
	putchar(10);

	putuint(sptr->x);
	putchar(10);

	return 0;
};
